  "bugs": {
    "url": "https://github.com/DavideDaniel/node-git-utils/issues"
  },
  "homepage": "https://github.com/DavideDaniel/node-git-utils#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "editme": "^1.1.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-json": "^2.0.0",
    "eslint-plugin-import": "^2.6.0",
    "version-bump-prompt": "^3.1.0"
  },
